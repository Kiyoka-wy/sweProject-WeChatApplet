<!--pages/task/published/published.wxml-->
<i-tabs current="{{ current }}" bindchange="handleChange">
    <i-tab key="tab1" title="未接受"></i-tab>
    <i-tab key="tab2" title="已接受"></i-tab>
    <i-tab key="tab3" title="已取消"></i-tab>
</i-tabs>

<i-panel>
    <view style="padding: 15px;">
      <i-panel class="cell-panel-demo">
        <i-cell title="任务标题" label="任务地点" value="任务时间" is-link></i-cell>
      </i-panel>
    
    </view>
    
    <i-button bind:click="handleClick" inline type="error">取消发布</i-button>
</i-panel>
